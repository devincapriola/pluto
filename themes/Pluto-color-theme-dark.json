	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"foreground": "#699955"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"string.quoted.raw",
				"string.quoted.triple",
				"string.unquoted",
				"string.other"
			],
			"settings": {
				"foreground": "#cd8b6e"
			}
		},
		{
			"name": "tags",
			"scope": [
				"punctuation.definition.tag",
				"entity.name.tag"
			],
			"settings": {
				"foreground": "#4d4d4d"
			}
		},
		{
			"name": "Storage",
			"scope": [
				"storage.type",
				"storage.modifier",
				"storage.class",
				"storage.function",
				"storage.variable",
				"storage.parameter"
			],
			"settings": {
				"foreground": "#448cce"
			}
		},
		{
			"name": "Number",
			"scope": [
				"constant.numeric"
			],
			"settings": {
				"foreground": "#adc6a2"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Ture & False",
			"scope": [
				"constant.language"
			],
			"settings": {
				"foreground": "#448cce"
			}
		},
		{
			"name": "KeyWord, Storage",
			"scope": [
				"keyword.control",
				"storage.type",
				"storage.modifier",
				"storage.class",
				"storage.function",
				"storage.variable"
			],
			"settings": {
				"foreground": "#c792ea"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name.type.class",
				"entity.name.function",
				"variable.function",
				"entity.name",
				"support.type",
				"support.class",
				"support.other.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#42c39e"
			}
		},
		{
			"name": "Json",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Tag name",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#448cce"
			}
		},
		{
			"name": "HTML Attributes",
			"scope": [
				"text.html.basic entity.other.attribute-name.html",
				"text.html.basic entity.other.attribute-name",
				"punctuation.definition.tag.html",
				"entity.other.attribute-name",
				"constant.character"
			],
			"settings": {
				"foreground": "#9cdcfe"
			}
		},
		{
			"name": "Function",
			"scope": [
				"keyword.other.special-method",
				"support.function",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#cedba9"
			}
		},
		{
			"name": "CSS Classes",
			"scope": [
				"entity.other.attribute-name.class"
			],
			"settings": {
				"foreground": "#FFCB6B"
			}
		},
		{
			"name": "CSS ID's",
			"scope": [
				"source.sass keyword.control"
			],
			"settings": {
				"foreground": "#82AAFF"
			}
		},
		{
			"name": "Escape Characters",
			"scope": [
				"constant.character.escape"
			],
			"settings": {
				"foreground": "#ffb400"
			}
		},
		{
			"name": "URL",
			"scope": [
				"*url*",
				"*link*",
				"*uri*"
			],
			"settings": {
				"fontStyle": "underline"
			}
		}
	]
}